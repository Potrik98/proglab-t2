<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proglab.t2.zumocontrol.Zumocontrol">
    <SplitPane dividerPositions="0.125" orientation="VERTICAL" prefHeight="400.0" prefWidth="600.0">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="50" prefWidth="600">
            <ButtonBar prefHeight="50.0" prefWidth="600">
                <buttons>
                    <Button onAction="#action1" mnemonicParsing="false" prefWidth="300" text="Action1" ButtonBar.buttonData="LEFT" />
                    <Button onAction="#action2" mnemonicParsing="false" prefWidth="300" text="Action2" ButtonBar.buttonData="RIGHT" />
                </buttons>
            </ButtonBar>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="350.0" prefWidth="600.0">
            <TextArea fx:id="logArea" prefHeight="350.0" prefWidth="600.0"/>
        </AnchorPane>
    </SplitPane>
</AnchorPane>
